 <div class="panel panel-default ">	
   <div class="panel-body">
     <form id="signupForm" method="POST" action="U_F002EXP.apw" class="form-horizontal">
	 <div class="form-group">
	      <label for="zz4_nembar" class="col-sm-1 control-label">N. Embarque</label>       
          <div class="col-sm-2 zz4_nembar">
            <input type="text" class="form-control" name="zz4_nembar" id="zz4_nembar"  maxlength="10" value="<%= Alltrim(zz4_nembar)%>">
          </div>          
          <label for="zz4_carreg" class="col-sm-2 control-label">Carregamento</label>
          <div class="col-sm-2 zz4_carreg">
            <select class="form-control" id="zz4_carreg" name="zz4_carreg">
                <option value="">Selecione</option>                
                <option value="EA" <%= IF((zz4_carreg=="EA"), "selected", "")%> >EXP Aereo</option>                
                <option value="EM" <%= IF((zz4_carreg=="EM"), "selected", "")%> >EXP Maritimo</option>                
                <option value="MI" <%= IF((zz4_carreg=="MI"), "selected", "")%> >Mercado Interno</option>                
            </select>
          </div>
         <label for="zz4_destin" class="col-sm-1 control-label">Destino</label>
          <div class="col-sm-2 zz4_destin">
            <select class="form-control" id="zz4_destin" name="zz4_destin">
                <option value="">Selecione</option>                
                <option value="EUR" <%= IF((zz4_destin=="EUR"), "selected", "")%> >Europa</option>                
                <option value="EUA" <%= IF((zz4_destin=="EUA"), "selected", "")%> >E.U.A</option>                
                <option value="MI"  <%= IF((zz4_destin=="MI".OR.zz4_destin==".MI"), "selected", "")%> >MI</option>                
            </select>
          </div>
     </div>
     
