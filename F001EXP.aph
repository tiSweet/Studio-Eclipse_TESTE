    <script type="text/javascript">
		function autoTab(input, e)  {   
		  var ind = 0;  
		  var isNN = (navigator.appName.indexOf("Netscape")!=-1);  
		  var keyCode = (isNN) ? e.which : e.keyCode;   
		  var nKeyCode = e.keyCode;   
		  if(keyCode == 13){
		  	e.preventDefault(e);   
			if (!isNN) {window.event.keyCode = 0;} // evitar o beep  
			ind = getIndex(input);  
			if (input.form[ind].type == 'textarea') {  
			  return;  
			}  
			ind++;  
			input.form[ind].focus();   
			if (input.form[ind].type == 'text') {  
			  input.form[ind].select();   
			}  
		  } 
		  
		  function getIndex(input) {   
			var index = -1, i = 0, found = false;   
			while (i < input.form.length && index == -1)   
			  if (input.form[i] == input) {  
				index = i;  
				  if (i < (input.form.length -1)) {  
				   if (input.form[i+1].type == 'hidden') {  
			   index++;   
			 }  
			 if (input.form[i+1].type == 'button' && input.form[i+1].id == 'tabstopfalse') {  
			   index++;   
			 }  
		   }  
			  }  
			  else   
		   i++;   
			return index;   
		  }  
		}  
		$('.well').on('keyup', 'input', function(event) {
		  if (event.which == 13) {
			var generico = $(".well").find('input:visible');
			var indice = generico.index(event.target) + 0;
			var seletor = $(generico[indice]).focus();
	
			if (seletor.length == 0) {
			  event.target.focus();
			}
		  }
		});
    </script>
    
