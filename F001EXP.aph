	<script type="text/javascript">
	function dataAtual(){ 
		data = new Date();
		datasaida = $('#zz4_dsaida').val();	
		var dia = data.getDate();
		dia = (dia.toString().length == 1)?'0'+dia:dia;
		var mes = data.getMonth() + 1;
		mes = (mes.toString().length == 1)?'0'+mes:mes;
		   if (datasaida == '') {
			document.getElementById('zz4_dsaida').value = dia +'/'+mes+'/'+data.getFullYear(); 
		   }
		
	} 	
	function horaAtual(){ 
		data = new Date();
		horasaida = $('#zz4_horas').val();	
		var hora = data.getHours();
		hora = (hora.toString().length == 1)?'0'+hora:hora;
		var minuto = data.getMinutes();
		minuto = (minuto.toString().length == 1)?'0'+minuto:minuto;
		   if (horasaida == '') {
			document.getElementById('zz4_horas').value = hora +':'+minuto;; 
		   }
		
	}  
	</script> 	
