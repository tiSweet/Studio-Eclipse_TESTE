			jQuery.validator.addMethod( "unique", function(value, element) {
				var parentForm = $(element).closest('.well');
				var timeRepeated = 0;
				if (value != '') {
					$(parentForm.find(':text')).each(function () {
						if ($(this).val() === value) {
							timeRepeated++;
						}
					});
				}
				return timeRepeated === 1 || timeRepeated === 0;
		
			}, "* Duplicate");
	
			jQuery.validator.addMethod( "embar", function(value, element) {
			    var EA = /[0-9]{4}[e-eE-E]{1}[a-aA-A]{1}[0-9]{4}/g;
				var EM = /[0-9]{4}[e-eE-E]{1}[m-mM-M]{1}[0-9]{4}/g;
				var MI = /[0-9]{4}[m-mM-M]{1}[i-iI-I]{1}[0-9]{4}/g;
				ea = '';
				em = '';
				mi = '';
				var embar;
				embar = '';				
				embar = $('#zz4_nembar').val()				
			    var is_valid = false;
			 
			      if (EA.test(embar) || EM.test(embar) || MI.test(embar)) {
					is_valid = true; 
				  } else {
					is_valid = false;
				  }
			   			 
			   return this.optional( element ) || is_valid ;
			 
			}, "Informe um número de embarque válido." );

			
